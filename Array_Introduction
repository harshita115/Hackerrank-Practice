/*include all the header files*/
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n, i;
   // Input by user 
  cin >> n;
  int a[n];
  for (i; i < n; i++)
    cin >> a[i];
  for (i; i >0; i--)
    cout << a[i- 1] <<" ";
  return 0;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
